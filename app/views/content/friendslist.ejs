

<div class="container">
   <table id="tabledata">
     <tr> 
     <th>First Name</th>
     <th>Last Name</th>
     <th>Title</th>
     <th>Birth</th>
     <th>Qalification</th>
     </tr>
     <% friends.forEach(function(item){ %>
      <tr>
       <td> <%=item.firstName%> </td>
       <td> <%=item.lastName%> </td>
       <td> <%=item.title%></td>
       <td><%=item.dateOfBirth%></td>
       <td><%=item.Qalification%></td>
      </tr>
     <% }); %>
    </table>
</div>
    
           
        
     
       
       
          
      
            
      
          
      
